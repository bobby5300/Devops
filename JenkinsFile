pipeline {

    agent any
    
    stages {

        stage('checkout from Dev Branch') {

            steps{

                checkout([
                    $class: 'GitSCM',
                    branches: [[name: '*/Dev']], 
                    extensions: [],
                    userRemoteConfigs: [[url: 'https://github.com/bobby5300/Devops.git']]
                      ])

            }
        }

        stage('Merge Dev to Master') {

            steps {
                
                sh 'git merge main'
            }

        }

            

        


    }

}
